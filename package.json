{
  "name": "yunaforseyfert",
  "version": "0.11.0",
  "description": "Yunathings for seyfert",
  "main": "build/index.js",
  "type": "commonjs",
  "types": "./types/package/index.d.ts",
  "scripts": {
    "build": "tsc && esbuild --bundle src/package/index.ts --minify --outdir=build --platform=node --packages=external",
    "tsc": "tsc",
    "lint": "biome lint --write ./src",
    "format": "biome check --write ./src",
    "test": "vitest",
    "prepare": "husky"
  },
  "keywords": [
    "parser",
    "penguin"
  ],
  "author": "SagiriIkeda",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.37.77",
    "seyfert": "github:SagiriIkeda/seyfert"
  },
  "homepage": "https://github.com/SagiriIkeda/yunaforseyfert#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SagiriIkeda/yunaforseyfert.git"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write",
      "biome format --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/node": "^20.11.30",
    "esbuild": "^0.20.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "typescript": "^5.0.0",
    "vitest": "^1.5.2"
  }
}
