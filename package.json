    "seyfert": "^1.2.0"
  },
  "homepage": "https://github.com/SagiriIkeda/yunaforseyfert#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SagiriIkeda/yunaforseyfert.git"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.0",
    "@types/node": "^20.11.30",
    "esbuild": "^0.20.2",
    "typescript": "^5.0.0",
    "vitest": "^1.5.2"
  }
}
